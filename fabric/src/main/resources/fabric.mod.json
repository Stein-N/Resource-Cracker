{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${mod_version}",
    "name": "${mod_name}",
    "description": "${mod_description}",
    "authors": [
        "${mod_author}"
    ],
    "contact": {
        "homepage": "https://modrinth.com/mod/resource-cracker",
        "sources": "https://github.com/Stein-N/Resource-Cracker",
        "issues": "https://github.com/Stein-N/Resource-Cracker/issues"
    },
    "license": "${mod_license}",
    "icon": "icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [ "net.xstopho.resource_cracker.ResourceCracker" ],
        "client": [ "net.xstopho.resource_cracker.ResourceCrackerClient" ],
        "fabric-datagen": [ "net.xstopho.resource_cracker.ResourceCrackerDatagen" ]
    },
    "mixins": [
        "${mod_id}.mixins.json",
        "${mod_id}.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.15",
        "fabric-api": "*",
        "minecraft": ">=1.20 <1.20.3",
        "java": ">=17",
        "stophoslib": ">=${stophos_lib}",
        "simpleconfigapi": ">=${simple_config_api}"
    },
    "suggests": {
        "another-mod": "*"
    }
}
  