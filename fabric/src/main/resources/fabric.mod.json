{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${description}",
    "authors": [
        "${mod_author}"
    ],
    "contact": {
        "homepage": "https://github.com/Stein-N/Resource-Cracker",
        "sources": "https://github.com/Stein-N/Resource-Cracker"
    },
    "license": "${license}",
    "icon": "icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "net.xstopho.resource_cracker.ResourceCracker"
        ],
        "client": [
            "net.xstopho.resource_cracker.ResourceCrackerClient"
        ],
        "fabric-datagen": [
            "net.xstopho.resource_cracker.ResourceCrackerDatagen"
        ]
    },
    "mixins": [
        "${mod_id}.mixins.json",
        "${mod_id}.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.15",
        "fabric": "*",
        "minecraft": ">=1.20.3",
        "java": ">=17",
        "simpleconfigapi": ">=0.1.2"
    },
    "suggests": {
        "another-mod": "*"
    }
}
  